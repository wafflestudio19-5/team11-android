<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:shareInterpolator="false" android:fillAfter="true" >
    <translate android:fromYDelta="0" android:toYDelta="0"
        android:duration="350"/>
    <alpha
        android:interpolator="@android:anim/accelerate_interpolator"
        android:fromAlpha="0.4" android:toAlpha="1.0"
        android:duration="350" />
    <scale
        android:duration="350"
        android:fromXScale="0.93"
        android:fromYScale="0.93"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXScale="1"
        android:toYScale="1" >
    </scale>
</set>